    "/Indicators/All": {
      "get": {
        "tags": [
          "Indicators"
        ],
        "summary": "Obtener la lista de indicadores",
        "description": "Este endpoint devuelve la lista de indicadores registrados.",
        "responses": {
          "200": {
            "description": "Respuesta exitosa",
            "content": {
              "application/json": {
                "example": [
                  {
                    "id": 1,
                    "name": "Proyecto A",
                    "description": "Desarrollo de una aplicación web",
                    "fecha_init": "2023-10-15",
                    "fecha_end": "2024-03-30",
                    "formula": "E = mc^2",
                    "frecuency": "Semanal",
                    "completation": 75,
                    "area": "Tecnología"
                  }
                ]
              }
            }
          },
          "404": {
            "description": "No se encontraron indicadores"
          }
        }
      }
    },
    "/Indicators/New": {
      "post": {
        "tags": [
          "Indicators"
        ],
        "summary": "Crear un nuevo indicador",
        "description": "Este endpoint crea un nuevo indicador.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  },
                  "fecha_init": {
                    "type": "string"
                  },
                  "fecha_end": {
                    "type": "string"
                  },
