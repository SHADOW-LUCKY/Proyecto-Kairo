                  },
                  "frecuency": {
                    "type": "string"
                  },
                  "completation": {
                    "type": "number"
                  },
                  "area": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Indicador creado con éxito"
          },
          "400": {
            "description": "Datos de indicador no válidos"
          }
        }
      }
    },
